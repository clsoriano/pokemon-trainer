                                    "label": "Documento*",
                                    "value": "",
                                    "validators": [
                                        {
                                            "type": "old",
                                            "message": "",
                                            "expression": "if(values.birthday && !value) {let diff = (new Date().getFullYear() - new Date(values.birthday.format('YYYY-MM-DD')).getFullYear()); if(isNaN(diff)) { return false; } if(diff >= 18){return true;} } return !values.birthday ? true : null;"
                                        }
                                    ],
                                    "show": true,
                                    "appearance": "outline",
